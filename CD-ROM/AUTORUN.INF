[AutoRun]
label=SYSTEMROOT
UseAutoPlay=1
Icon=\system32\res\32.ico

[AutoRun.AMD64]
label=SYSTEMROOT
UseAutoPlay=1
Icon=\system32\res\64.ico

[Content]
MusicFiles=0
PictureFiles=0
VideoFiles=0

[IgnoreContentPaths]
\System32

[DeviceInstall]
DriverPath=\system32\drivers\usb\x86\
DriverPath=\system32\drivers\usb\x64\
